@prefix mnd-s: <http://semantic-machines.com/veda/mondi-schema/> .
@prefix mnd-s-pass: <http://semantic-machines.com/veda/mondi-schema-pass/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://semantic-machines.com/veda/mondi-schema-pass>
  rdf:type owl:Ontology ;
  rdfs:label "Mondi ontology. Pass"@en ;
  rdfs:label "Онтология Монди. Пропуска"@ru ;
#  owl:versionInfo "1.0" ;
  v-s:loadPriority 10 ;
.
mnd-s-pass:OffenceAct
  rdf:type owl:Class ;
  rdfs:label "Offence act"@en ;
  rdfs:label "Акт нарушения"@ru ;
  rdfs:subClassOf v-s:Document ;
.
mnd-s-pass:OffenceActKind
  rdf:type owl:Class ;
  rdfs:label "Offence act kind"@en ;
  rdfs:label "Вид акта нарушения"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s-pass:PassKind
  rdf:type owl:Class ;
  rdfs:label "Pass kind"@en ;
  rdfs:label "Вид пропуска"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s-pass:PassRequest
  rdf:type owl:Class ;
  rdfs:label "Pass request"@en ;
  rdfs:label "Заявка на пропуск"@ru ;
  rdfs:subClassOf v-s:Document ;
.
mnd-s-pass:PassRequestElement
  rdf:type owl:Class ;
  rdfs:label "Pass request element"@en ;
  rdfs:label "Элемент заявки на пропуск"@ru ;
  rdfs:subClassOf v-s:Document ;
.
mnd-s-pass:Timetable
  rdf:type owl:Class ;
  rdfs:label "Timetable"@en ;
  rdfs:label "График работы"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s-pass:VehicleKind
