  rdfs:label "марка транспортного средства"@ru ;
  rdfs:range mnd-s-pass:VehicleLabel ;
.
mnd-s-pass:hasVehicleOwner
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s-pass:PassRequestElement ;
