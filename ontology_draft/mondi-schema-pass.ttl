  rdfs:label "Vehicle kind"@en ;
  rdfs:label "Категория транспортного средства"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s-pass:VehicleLabel
  rdf:type owl:Class ;
  rdfs:label "Vehicle label"@en ;
  rdfs:label "Марка транспортного средства"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s-pass:equipment
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s-pass:PassRequestElement ;
  rdfs:label "equipment"@en ;
  rdfs:label "оборудование"@ru ;
  rdfs:range xsd:string ;
.
mnd-s-pass:hasKpp
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s-pass:PassRequestElement ;
  rdfs:label "kpp"@en ;
  rdfs:label "кпп"@ru ;
  rdfs:range mnd-s:Area ;
.
mnd-s-pass:hasOffenceActKind
