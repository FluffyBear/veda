  rdfs:domain mnd-s-pass:PassRequestElement ;
.
mnd-s:hasCheckedDepartment
  rdfs:domain mnd-s-pass:PassRequestElement ;
.
mnd-s:hasOrganizationFrom
  rdfs:domain mnd-s-pass:OffenceAct ;
  rdfs:domain mnd-s-pass:PassRequest ;
  rdfs:domain mnd-s-pass:PassRequestElement ;
.
mnd-s:hasOrganizationTo
  rdfs:domain mnd-s-pass:PassRequestElement ;
.
mnd-s:initiatorDepartment
  rdfs:domain mnd-s-pass:PassRequest ;
  rdfs:domain mnd-s-pass:PassRequestElement ;
