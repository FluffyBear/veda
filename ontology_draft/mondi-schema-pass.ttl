  rdfs:label "вид пропуска"@ru ;
  rdfs:range mnd-s-pass:PassKind ;
.
mnd-s-pass:hasPassRequestElement
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s-pass:PassRequest ;
  rdfs:label "pass request element"@en ;
  rdfs:label "элемент заявки на пропуск"@ru ;
  rdfs:range mnd-s-pass:PassRequestElement ;
.
mnd-s-pass:hasTimetable
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s-pass:PassRequestElement ;
