  rdfs:label "дата публикации"@ru ;
  rdfs:range xsd:dateTime ;
.
veda:script
  rdf:type owl:DatatypeProperty ;
  rdfs:domain veda:Mandate ;
  rdfs:label "script"@en ;
  rdfs:label "скрипт"@ru ;
  rdfs:range xsd:string ;
.
veda:startTime
  rdf:type owl:DatatypeProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          veda:HRRecord
          veda:Delegation
          veda:Permission
        ) ;
    ] ;
  rdfs:label "start time"@en ;
  rdfs:label "время начала"@ru ;
  rdfs:range xsd:dateTime ;
.
veda:userName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain veda:Account ;
  rdfs:label "user name"@en ;
  rdfs:label "имя пользователя"@ru ;
  rdfs:range xsd:string ;
.
rdfs:label
  rdfs:label "наименование"@ru ;
.
