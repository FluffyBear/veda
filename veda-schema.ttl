# baseURI: http://semantic-machines.com/veda/veda-schema
# imports: http://www.w3.org/1999/02/22-rdf-syntax-ns#
# imports: http://www.w3.org/2000/01/rdf-schema#
# imports: http://www.w3.org/2002/07/owl

# c14n-version: 3
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix veda: <http://semantic-machines.com/veda/veda-schema/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
<http://semantic-machines.com/veda/veda-schema>
  rdf:type owl:Ontology ;
  owl:imports rdf: ;
  owl:imports rdfs: ;
  owl:imports <http://www.w3.org/2002/07/owl> ;
  owl:versionInfo "1.0" ;
.
veda:Absence
  rdf:type owl:Class ;
  rdfs:label "Absence"@en ;
  rdfs:label "Отсутствие"@ru ;
  rdfs:subClassOf veda:HRRecord ;
  owl:disjointWith veda:Appointment ;
.
veda:Account
  rdf:type owl:Class ;
  rdfs:label "Account"@en ;
  rdfs:label "Учетная запись"@ru ;
.
veda:Appointment
  rdf:type owl:Class ;
  rdfs:label "Presence"@en ;
  rdfs:label "Присутствие"@ru ;
  rdfs:subClassOf veda:HRRecord ;
  owl:disjointWith veda:Absence ;
.
veda:AuthorizationStatement
  rdf:type owl:Class ;
  rdfs:label "Authorization record"@en ;
  rdfs:label "Запись авторизации"@ru ;
  rdfs:subClassOf veda:Document ;
.
veda:Delegation
  rdf:type owl:Class ;
  rdfs:label "Delegation record"@en ;
  rdfs:label "Запись о делегировании"@ru ;
  rdfs:subClassOf veda:AuthorizationStatement ;
.
veda:Department
  rdf:type owl:Class ;
  rdfs:comment "Department"@en ;
  rdfs:comment "Структурное подразделение организации"@ru ;
  rdfs:label "Department"@en ;
  rdfs:label "Подразделение"@ru ;
  rdfs:subClassOf veda:OrganizationUnit ;
  owl:disjointWith veda:Organization ;
  owl:disjointWith veda:Position ;
.
veda:Dictionary
  rdf:type owl:Class ;
  rdfs:comment "Dictionary"@en ;
  rdfs:comment "Словарь"@ru ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "Dictionary"@en ;
  rdfs:label "Словарь"@ru ;
  rdfs:subClassOf veda:Document ;
.
veda:Document
  rdf:type owl:Class ;
  rdfs:comment "A document"@en ;
  rdfs:comment "Документ"@ru ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "Document"@en ;
  rdfs:label "Документ"@ru ;
  rdfs:subClassOf owl:Thing ;
.
veda:Group
  rdf:type owl:Class ;
  rdfs:comment "Group of resources"@en ;
  rdfs:comment "Группа ресурсов"@ru ;
  rdfs:label "Group"@en ;
  rdfs:label "Группа"@ru ;
  rdfs:subClassOf owl:Thing ;
.
veda:HRRecord
  rdf:type owl:Class ;
  rdfs:label "HR record"@ru ;
  rdfs:label "Кадровая запись"@en ;
  rdfs:subClassOf veda:Document ;
.
veda:Mandate
  rdf:type owl:Class ;
  rdfs:label "Mandate"@en ;
  rdfs:label "Мандат"@ru ;
  rdfs:subClassOf veda:AuthorizationStatement ;
.
veda:Organization
  rdf:type owl:Class ;
  rdfs:comment "An organization"@en ;
  rdfs:comment "Организация, юридическое лицо"@ru ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "Organization"@en ;
  rdfs:label "Организация"@ru ;
  rdfs:subClassOf veda:OrganizationUnit ;
  owl:disjointWith veda:Department ;
  owl:disjointWith veda:Position ;
.
veda:OrganizationUnit
  rdf:type owl:Class ;
  rdfs:comment "An organization unit (organization, department, business unit etc.)"@en ;
  rdfs:comment "Организационная единица (организация, подразделение, штатная должность)"@ru ;
  rdfs:label "Organization unit"@en ;
  rdfs:label "Организационная единица"@ru ;
.
veda:Permission
  rdf:type owl:Class ;
  rdfs:comment "Permissions record defines access rigths for individuals of one class to individuals of another class."@en ;
  rdfs:comment "Запись о выдаче прав описывает права индивидов одного класса по отношению к индивидам другого класса."@ru ;
  rdfs:label "Permissions record"@en ;
  rdfs:label "Запись о выдаче прав"@ru ;
  rdfs:subClassOf veda:AuthorizationStatement ;
.
veda:Person
  rdf:type owl:Class ;
  rdfs:comment "A person." ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "Person" ;
  rdfs:subClassOf veda:Versioned ;
  rdfs:subClassOf owl:Thing ;
.
veda:Position
  rdf:type owl:Class ;
  rdfs:label "Position"@en ;
  rdfs:label "Должность"@ru ;
  rdfs:subClassOf veda:OrganizationUnit ;
  owl:disjointWith veda:Department ;
  owl:disjointWith veda:Organization ;
.
veda:Versioned
  rdf:type owl:Class ;
  rdfs:label "Versioned thing"@en ;
  rdfs:label "Версионная сущность"@ru ;
  rdfs:subClassOf owl:Thing ;
.
veda:accountOwner
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Account ;
  rdfs:label "account owner"@en ;
  rdfs:label "владелец учетной записи"@ru ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          veda:Person
          veda:OrganizationUnit
        ) ;
    ] ;
.
veda:actualVersion
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Versioned ;
  rdfs:label "actual version"@en ;
  rdfs:label "актуальная версия"@ru ;
  rdfs:range veda:Versioned ;
.
veda:author
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Document ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "author"@en ;
  rdfs:label "автор"@ru ;
  rdfs:range veda:Appointment ;
.
veda:birthday
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:comment "The birthday of this Person."@en ;
  rdfs:comment "День рождения этой персоны"@ru ;
  rdfs:domain veda:Person ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "birthday"@en ;
  rdfs:label "день рождения"@ru ;
  rdfs:range xsd:date ;
.
veda:childUnit
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:OrganizationUnit ;
  rdfs:label "child unit"@en ;
  rdfs:label "дочерняя единица"@ru ;
  rdfs:range veda:OrganizationUnit ;
.
veda:created
  rdf:type owl:DatatypeProperty ;
  rdfs:domain veda:Document ;
  rdfs:label "Creation date"@en ;
  rdfs:label "Дата создания"@ru ;
  rdfs:range xsd:dateTime ;
.
veda:delegateFrom
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Delegation ;
  rdfs:label "who delegates"@en ;
  rdfs:label "кто делегирует"@ru ;
  rdfs:range veda:OrganizationUnit ;
.
veda:delegateTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Delegation ;
  rdfs:label "to whom delegates"@en ;
  rdfs:label "кому делегирует"@ru ;
  rdfs:range veda:Person ;
.
veda:employee
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:HRRecord ;
  rdfs:label "employee"@en ;
  rdfs:label "работник"@ru ;
  rdfs:range veda:Person ;
.
veda:endTime
  rdf:type owl:DatatypeProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          veda:HRRecord
          veda:Delegation
          veda:Permission
        ) ;
    ] ;
  rdfs:label "end time"@en ;
  rdfs:label "время окончания"@ru ;
  rdfs:range xsd:dateTime ;
.
veda:firstName
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The first name of a person."@en ;
  rdfs:comment "Имя персоны."@ru ;
  rdfs:domain veda:Person ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "firstName"@en ;
  rdfs:label "имя"@ru ;
  rdfs:range rdfs:Literal ;
.
veda:hasAccount
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          veda:Person
          veda:OrganizationUnit
        ) ;
    ] ;
  rdfs:label "account"@en ;
  rdfs:label "учетная запись"@ru ;
  rdfs:range veda:Account ;
  owl:inverseOf veda:accountOwner ;
.
veda:lastName
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The last name of a person."@en ;
  rdfs:comment "Фамилия персоны"@ru ;
  rdfs:domain veda:Person ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "lastName"@en ;
  rdfs:label "фамилия"@en ;
  rdfs:range rdfs:Literal ;
.
veda:mailbox
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Mailbox"@en ;
  rdfs:comment "Адрес электронной почты"@ru ;
  rdfs:domain veda:Account ;
  rdfs:isDefinedBy veda: ;
  rdfs:label "mailbox"@en ;
  rdfs:label "адрес эл. почты"@ru ;
  rdfs:range xsd:string ;
.
veda:memberOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          veda:OrganizationUnit
          veda:Person
          veda:Document
        ) ;
    ] ;
  rdfs:label "member of group"@en ;
  rdfs:label "член группы"@ru ;
  rdfs:range veda:Group ;
.
veda:middleName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain veda:Person ;
  rdfs:label "middle name"@en ;
  rdfs:label "отчество"@ru ;
  rdfs:range xsd:string ;
.
veda:nextVersion
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Versioned ;
  rdfs:label "next version"@en ;
  rdfs:label "следующая версия"@ru ;
  rdfs:range veda:Versioned ;
.
veda:occupation
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Appointment ;
  rdfs:label "occupation"@en ;
  rdfs:label "занимает должность"@ru ;
  rdfs:range veda:OrganizationUnit ;
.
veda:official
  rdf:type rdf:Property ;
  rdfs:domain veda:Appointment ;
  rdfs:label "official"@en ;
  rdfs:label "официально"@ru ;
  rdfs:range xsd:boolean ;
.
veda:parentUnit
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:OrganizationUnit ;
  rdfs:label "parent unit"@en ;
  rdfs:label "родительская единица"@ru ;
  rdfs:range veda:OrganizationUnit ;
.
veda:password
  rdf:type owl:DatatypeProperty ;
  rdfs:domain veda:Account ;
  rdfs:label "password"@en ;
  rdfs:label "пароль"@ru ;
  rdfs:range xsd:string ;
.
veda:permission
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "A string defining permissions: [+|-]C[+|-]R[+|-]U[+|-]D."@en ;
  rdfs:comment "Строка определяющая уровень прав: [+|-]C[+|-]R[+|-]U[+|-]D."@ru ;
  rdfs:domain veda:Permission ;
  rdfs:label "permissions"@en ;
  rdfs:label "права"@ru ;
  rdfs:range xsd:string ;
.
veda:permissionObject
  rdf:type owl:ObjectProperty ;
  rdfs:comment "A resource to which rights are granted."@en ;
  rdfs:comment "Ресурс, на который выдаются права."@ru ;
  rdfs:domain veda:Permission ;
  rdfs:label "permission resource"@en ;
  rdfs:label "ресурс полномочий"@ru ;
  rdfs:range veda:Group ;
.
veda:permissionSubject
  rdf:type owl:ObjectProperty ;
  rdfs:comment "Agent whom rights are granted."@en ;
  rdfs:comment "Агент, которому предоставляются права."@ru ;
  rdfs:domain veda:Permission ;
  rdfs:label "permission agent"@en ;
  rdfs:label "агент полномочий"@ru ;
  rdfs:range veda:Group ;
.
veda:previousVersion
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda:Versioned ;
  rdfs:label "previous version"@en ;
  rdfs:label "предыдущая версия"@ru ;
  rdfs:range veda:Versioned ;
.
veda:published
  rdf:type owl:DatatypeProperty ;
  rdfs:domain veda:Document ;
