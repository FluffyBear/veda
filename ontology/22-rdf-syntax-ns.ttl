@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rdf:Alt a rdfs:Class ;
    rdfs:label "Alt"@en ;
    rdfs:comment "The class of containers of alternatives."@en ;
    rdfs:isDefinedBy rdf: ;
    rdfs:subClassOf rdfs:Container .

rdf:Bag a rdfs:Class ;
    rdfs:label "Bag"@en ;
    rdfs:comment "The class of unordered containers."@en ;
    rdfs:isDefinedBy rdf: ;
    rdfs:subClassOf rdfs:Container .

rdf:List a rdfs:Class ;
    rdfs:label "List"@en ;
    rdfs:comment "The class of RDF Lists."@en ;
    rdfs:isDefinedBy rdf: ;
    rdfs:subClassOf rdfs:Resource .

rdf:Property a rdfs:Class ;
    rdfs:label "Property"@en ;
    rdfs:label "Свойство"@ru ;
    rdfs:comment "The class of RDF properties."@en ;
    rdfs:isDefinedBy rdf: ;
    rdfs:subClassOf rdfs:Resource .

rdf:Seq a rdfs:Class ;
    rdfs:label "Seq"@en ;
    rdfs:comment "The class of ordered containers."@en ;
    rdfs:isDefinedBy rdf: ;
    rdfs:subClassOf rdfs:Container .

rdf:Statement a rdfs:Class ;
    rdfs:label "Statement"@en ;
    rdfs:label "Утверждение"@ru ;
    rdfs:comment "The class of RDF statements."@en ;
    rdfs:isDefinedBy rdf: ;
    rdfs:subClassOf rdfs:Resource .

rdf:PlainLiteral a rdfs:Datatype ;
    rdfs:label "Plain literal"@en ;
    rdfs:label "Простой литерал"@ru ;
    rdfs:comment "The class of plain (i.e. untyped) literal values."@en ;
    rdfs:isDefinedBy <http://www.w3.org/TR/rdf-plain-literal/> ;
    rdfs:subClassOf rdfs:Literal .

rdf:XMLLiteral a rdfs:Datatype ;
    rdfs:label "XMLLiteral"@en ;
    rdfs:label "XML литерал"@ru ;
    rdfs:comment "The class of XML literal values."@en ;
    rdfs:isDefinedBy rdf: ;
    rdfs:subClassOf rdfs:Literal .

rdf:first a rdf:Property ;
    rdfs:label "first"@en ;
    rdfs:comment "The first item in the subject RDF list."@en ;
    rdfs:domain rdf:List ;
    rdfs:isDefinedBy rdf: ;
    rdfs:range rdfs:Resource .

() a rdf:List ;
    rdfs:label "nil"@en ;
    rdfs:comment "The empty list, with no items in it. If the rest of a list is nil then the list has no more items in it."@en ;
    rdfs:isDefinedBy rdf: .

rdf:object a rdf:Property ;
    rdfs:label "object"@en ;
    rdfs:comment "The object of the subject RDF statement."@en ;
    rdfs:domain rdf:Statement ;
    rdfs:isDefinedBy rdf: ;
    rdfs:range rdfs:Resource .

rdf:predicate a rdf:Property ;
    rdfs:label "predicate"@en ;
    rdfs:comment "The predicate of the subject RDF statement."@en ;
    rdfs:domain rdf:Statement ;
    rdfs:isDefinedBy rdf: ;
    rdfs:range rdfs:Resource .

rdf:rest a rdf:Property ;
    rdfs:label "rest"@en ;
    rdfs:comment "The rest of the subject RDF list after the first item."@en ;
    rdfs:domain rdf:List ;
    rdfs:isDefinedBy rdf: ;
    rdfs:range rdf:List .

rdf:subject a rdf:Property ;
    rdfs:label "subject"@en ;
    rdfs:comment "The subject of the subject RDF statement."@en ;
    rdfs:domain rdf:Statement ;
    rdfs:isDefinedBy rdf: ;
    rdfs:range rdfs:Resource .

rdf:type a rdf:Property ;
    rdfs:label "type"@en ;
    rdfs:label "тип"@ru ;
    rdfs:comment "The subject is an instance of a class."@en ;
    rdfs:domain rdfs:Resource ;
    rdfs:isDefinedBy rdf: ;
    rdfs:range rdfs:Class .

rdf:value a rdf:Property ;
    rdfs:label "value"@en ;
    rdfs:comment "Idiomatic property used for structured values."@en ;
    rdfs:domain rdfs:Resource ;
    rdfs:isDefinedBy rdf: ;
    rdfs:range rdfs:Resource .

rdf: a owl:Ontology ;
    dc:description "This is the RDF Schema for the RDF vocabulary defined in the RDF namespace."@en ;
    dc:title "The RDF Vocabulary (RDF)"@en ;
    rdfs:label "The RDF Vocabulary (RDF)"@en ;
    rdfs:label "Словарь RDF"@ru ;
    rdfs:seeAlso <http://www.w3.org/2000/01/rdf-schema-more> .

