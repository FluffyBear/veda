v-s:ContractorProfileFileTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон для класса v-s:ContractorProfileFile"@ru ;
  rdfs:label "Template for class v-s:ContractorProfileFile"@en ;
  v-ui:forClass v-s:ContractorProfileFile ;
  v-ui:template """
<div class="panel panel-default" style="word-wrap:break-word; width:250px; display: inline-block; margin:0 20px 20px 0; overflow: hidden">
  <div class="panel-body">
    <em about="v-s:hasFileContentClassifier" property="rdfs:label" class="-view edit search"></em>
    <strong rel="v-s:hasFileContentClassifier" class="view -edit -search" data-template="v-ui:LabelTemplate"></strong>
    <veda-control data-type="link" rel="v-s:hasFileContentClassifier" class="-view edit search fulltext dropdown"></veda-control>

    <em about="rdfs:comment" property="rdfs:label" class="-view edit search"></em>
    <span property="rdfs:comment" class="view -edit -search"></span>
    <veda-control data-type="string" property="rdfs:comment" class="-view edit search"></veda-control>
    <br>
    <hr class="margin-sm view -edit -search">
    <div>
      <span id="icon" class="label label-primary"></span>
      <a href="/files/@">
        <span about="@" property="v-s:fileName"></span>
      </a>
    </div>
    <em about="v-s:dateFrom" property="rdfs:label"></em>
    <div property="v-s:dateFrom" class="view -edit -search"></div>
    <veda-control property="v-s:dateFrom" data-type="dateTime" class="-view edit search"></veda-control>

    <em about="v-s:dateTo" property="rdfs:label"></em>
    <div property="v-s:dateTo" class="view -edit -search"></div>
    <veda-control property="v-s:dateTo" data-type="dateTime" class="-view edit search"></veda-control>

    <div class="view -edit -search" style="font-style:italic">
      <small rel="v-s:creator" data-template="v-ui:LabelTemplate"></small><small property="v-s:created"></small>
    </div>
  </div>
</div>
<script>
  var fn = individual["v-s:fileName"][0],
    idx = fn.lastIndexOf("."),
    ext = fn.substr(idx + 1);
  $("span#icon", template).text(ext);
  //# sourceURL=v-s:ContractorProfileFileTemplate
</script>
  """ ;
.
