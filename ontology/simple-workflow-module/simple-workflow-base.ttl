        <button type="button" class="action btn btn-success" id="save" about="v-s:Save" property="rdfs:label"/>
        <button type="button" class="action btn btn-success" id="save_and_start_process" about="v-s:Send" property="rdfs:label"/>
        <button type="button" class="action btn btn-default" id="cancel" about="v-s:Cancel" property="rdfs:label"/>
        <button type="button" class="action btn btn-link" id="delete" about="v-s:Delete" property="rdfs:label"/>
      </div>
    </div>
  </div>
<script type="text/javascript">
  var stages = [
    'informing',
    'coordinator1',
    'coordinator2',
    'coordinator3',
    'coordinator4',
    'coordinator5',
