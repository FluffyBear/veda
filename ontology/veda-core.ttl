@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-fs: <http://semantic-machines.com/veda/veda-search/> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://semantic-machines.com/veda/veda-core>
  rdf:type owl:Ontology ;
  rdfs:label "Veda system ontology"@en ;
  rdfs:label "Онтология системы Veda"@ru ;
#  owl:versionInfo "1.3" ;
  v-s:loadPriority 6 ;
.

### CLASSES ###

v-s:Bundle
  rdf:type owl:Class ;
  rdfs:label "Bundle"@en ;
  rdfs:label "Текстовая константа"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:Account
  rdf:type owl:Class ;
  rdfs:label "Account"@en ;
  rdfs:label "Учетная запись"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:Email
  rdf:type owl:Class ;
  rdfs:label "e-mail"@en ;
  rdfs:label "электронное письмо"@ru ;
  rdfs:subClassOf v-s:Deliverable ;
.

v-s:Codelet
  rdf:type owl:Class ;
  rdfs:label "Codelet"@en ;
  rdfs:label "Исполняемый Код"@ru ;
.

v-s:Container
  rdf:type owl:Class ;
  rdfs:label "Контейнер"@ru ;
.

v-s:TemplateForText
  rdf:type owl:Class ;
  rdfs:label "Шаблон для текста"@ru ;
.

v-s:Event
  rdf:type owl:Class ;
  rdfs:label "Event"@en ;
  rdfs:label "Событие"@ru ;
.

v-s:Group
  rdf:type owl:Class ;
  rdfs:comment "Group of resources"@en ;
  rdfs:comment "Группа ресурсов"@ru ;
  rdfs:label "Group"@en ;
  rdfs:label "Группа"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:Membership
  rdf:type owl:Class ;
  rdfs:label "Membership"@en ;
  rdfs:label "Принадлежность"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:PermissionFilter
  rdf:type owl:Class ;
  rdfs:comment "Запись о соответствии документа и фильтра записей о правах."@ru ;
  rdfs:label "Permissions filter record"@en ;
  rdfs:label "Фильтр прав"@ru ;
.

v-s:PermissionStatement
  rdf:type owl:Class ;
  rdfs:comment "Permissions record defines access rigths for individuals of one class to individuals of another class."@en ;
  rdfs:comment "Запись о выдаче прав описывает права индивидов одного класса по отношению к индивидам другого класса."@ru ;
  rdfs:label "Permissions record"@en ;
  rdfs:label "Запись о выдаче прав"@ru ;
.

v-s:Person
  rdf:type owl:Class ;
  rdfs:comment "A person." ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "Person"@en ;
  rdfs:label "Персона"@ru ;
  rdfs:subClassOf v-s:Versioned ;
  rdfs:subClassOf v-s:Journaling ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf v-s:Exportable ;  
.

v-s:Transform
  rdf:type owl:Class ;
  rdfs:label "для переливки из старой системы"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:Versioned
  rdf:type owl:Class ;
  rdfs:label "Versioned thing"@en ;
  rdfs:label "Версионная сущность"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:DraftAllowed
  rdf:type owl:Class ;
  rdfs:label "Draft allowed"@en ;
  rdfs:label "Возможно создание черновика"@ru ;
  rdfs:subClassOf v-s:Versioned ;
.

v-s:Exportable
  rdf:type owl:Class ;
  rdfs:label "Exportable thing"@en ;
  rdfs:label "Выгружаемая сущность"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:Deliverable
  rdf:type owl:Class ;
  rdfs:label "Сущность подлежащая доставке" ;
  rdfs:label "Exportable thing"@en ;
  rdfs:label "Экспортируемая сущность"@ru ;
  rdfs:subClassOf owl:Thing ;
.

v-s:AuthorizedClass
  rdf:type owl:Class ;
  rdfs:label "Authorized class"@en ;
  rdfs:label "Авторизуемый класс"@ru ;
  rdfs:comment "Суперкласс всех классов, индивиды которых могут авторизоваться по типу"@en ;
  rdfs:comment "Superclass for classes whose individuals should be authorized by type"@ru ;
.

v-s:ClassModel
  rdf:type owl:Class ;
  rdfs:label "Class model"@en ;
  rdfs:label "Модель класса"@ru ;
.

### PROPERTIES ###

v-s:hasDraft
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:DraftAllowed ;
  rdfs:label "has draft"@en ;
  rdfs:label "имеет черновик"@ru ;
  rdfs:range v-s:DraftAllowed ;
.

v-s:isDraftOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:DraftAllowed ;
  rdfs:label "is draft of"@en ;
  rdfs:label "является черновиком"@ru ;
  rdfs:range v-s:DraftAllowed ;
.

v-s:actualVersion
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:Versioned ;
  rdfs:label "actual version"@en ;
  rdfs:label "актуальная версия"@ru ;
  rdfs:range v-s:Versioned ;
.

v-s:author
  rdf:type owl:ObjectProperty ;
  rdfs:domain rdfs:Resource ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "author"@en ;
  rdfs:label "автор"@ru ;
  rdfs:range v-s:Appointment ;
.

v-s:birthday
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:comment "The birthday of this Person."@en ;
  rdfs:comment "День рождения этой персоны"@ru ;
  rdfs:domain v-s:Person ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "birthday"@en ;
  rdfs:label "день рождения"@ru ;
  rdfs:range xsd:dateTime ;
.

v-s:canCreate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:PermissionStatement ;
  rdfs:label "can create"@en ;
  rdfs:label "можно создавать"@ru ;
  rdfs:range xsd:boolean ;
.

v-s:canDelete
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:PermissionStatement ;
  rdfs:label "can delete"@en ;
  rdfs:label "можно удалять"@ru ;
  rdfs:range xsd:boolean ;
.

v-s:canRead
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:PermissionStatement ;
  rdfs:label "can read"@en ;
  rdfs:label "можно читать"@ru ;
  rdfs:range xsd:boolean ;
.

v-s:canUpdate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:PermissionStatement ;
  rdfs:label "can update"@en ;
  rdfs:label "можно изменять"@ru ;
  rdfs:range xsd:boolean ;
.

v-s:created
  rdf:type owl:DatatypeProperty ;
  rdfs:domain rdfs:Resource ;
  rdfs:label "entry date"@en ;
  rdfs:label "дата создания"@ru ;
  rdfs:range xsd:dateTime ;
.

v-s:deleted
  rdf:type owl:DatatypeProperty ;
  rdfs:domain rdfs:Resource ;
  rdfs:label "deleted"@en ;
  rdfs:label "удален"@ru ;
  rdfs:range xsd:boolean ;
.

v-s:destClass
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:Transform ;
  rdfs:label "название класса, в который вписываем"@ru ;
  rdfs:range xsd:string ;
.

v-s:destProperty
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:Transform ;
  rdfs:label "название свойства, в которое вписываем"@ru ;
  rdfs:range xsd:string ;
.

v-s:filter
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:Event ;
  rdfs:label "filter"@en ;
  rdfs:label "фильтр"@ru ;
  rdfs:range rdfs:Resource ;
.

v-s:firstName
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The first name of a person."@en ;
  rdfs:comment "Имя персоны."@ru ;
  rdfs:domain v-s:Person ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "first name"@en ;
  rdfs:label "имя"@ru ;
  rdfs:range xsd:string ;
.

v-s:fullUrl
  rdf:type owl:DatatypeProperty ;
  rdfs:domain owl:Ontology ;
  rdfs:label "full URL"@en ;
  rdfs:label "полный URL"@ru ;
  rdfs:range xsd:string ;
.

v-s:hasAccount
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:OrganizationUnit ;
  rdfs:domain v-s:Person ;
  rdfs:label "account"@en ;
  rdfs:label "учетная запись"@ru ;
  rdfs:range v-s:Account ;
.

v-s:lastName
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The last name of a person."@en ;
  rdfs:comment "Фамилия персоны"@ru ;
  rdfs:domain v-s:Person ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "last name"@en ;
  rdfs:label "фамилия"@ru ;
  rdfs:range xsd:string ;
.

v-s:login
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:Account ;
  rdfs:label "login"@en ;
  rdfs:label "логин"@ru ;
  rdfs:range xsd:string ;
.

v-s:mailbox
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Mailbox"@en ;
  rdfs:comment "Адрес электронной почты"@ru ;
  rdfs:domain v-s:Account ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "mailbox"@en ;
  rdfs:label "адрес эл. почты"@ru ;
  rdfs:range xsd:string ;
.

v-s:memberOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:Membership ;
  rdfs:label "member of group"@en ;
  rdfs:label "член группы"@ru ;
  rdfs:range v-s:Group ;
.

v-s:middleName
  rdf:type owl:DatatypeProperty ;
