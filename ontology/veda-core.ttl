  rdfs:label "отчество"@ru ;
  rdfs:range xsd:string ;
.

v-s:nextVersion
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:Versioned ;
  rdfs:label "next version"@en ;
  rdfs:label "следующая версия"@ru ;
  rdfs:range v-s:Versioned ;
.

v-s:owner
  rdf:type owl:ObjectProperty ;
