  rdfs:range xsd:string ;
.

v-s:templateSubject
  rdf:type owl:DatatypeProperty ;
  rdfs:label "тема шаблона" ;
  rdfs:range v-s:TemplateForText ;
  rdfs:range xsd:string ;
.

v-s:templateLanguage
  rdf:type owl:ObjectProperty ;
  rdfs:range v-s:TemplateForText ;
