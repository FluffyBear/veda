.

v-s:permissionSubject
  rdf:type owl:ObjectProperty ;
  rdfs:comment "Subject whom rights are granted."@en ;
  rdfs:comment "Субъект, которому предоставляются права."@ru ;
  rdfs:domain v-s:PermissionStatement ;
