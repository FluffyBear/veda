  rdfs:label "время начала"@ru ;
  rdfs:range xsd:dateTime ;
.

v-s:endTime
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:HRRecord ;
  rdfs:domain v-s:PermissionStatement ;
  rdfs:label "end time"@en ;
  rdfs:label "время окончания"@ru ;
  rdfs:range xsd:dateTime ;
.

