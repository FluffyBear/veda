@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix v-asp: <http://semantic-machines.com/veda/veda-aspect/> .
@prefix v-p: <http://semantic-machines.com/veda/veda-main/> .
@prefix v-fc: <http://semantic-machines.com/veda/veda-function-create/> .
@prefix v-fs: <http://semantic-machines.com/veda/veda-function-search/> .
@prefix v-ft: <http://semantic-machines.com/veda/veda-function-tasks/> .
@prefix d: <http://semantic-machines.com/veda/veda-data/> .

<http://semantic-machines.com/veda/veda-page>
  rdf:type owl:Ontology ;
  rdfs:label "Veda system main page ontology"@en ;
  rdfs:label "Онтология главной страницы системы Веда"@ru ;
  owl:imports <http://semantic-machines.com/veda/veda-schema> ;
  owl:imports <http://semantic-machines.com/veda/veda-ui> ;
  owl:imports rdf: ;
  owl:imports rdfs: ;
  owl:imports <http://www.w3.org/2002/07/owl> ;
#  owl:versionInfo "0.2" ;
.
v-p:Main
	rdf:type v-p:Page ;
	rdfs:label "Main page"@en ;
	rdfs:label "Главная страница"@ru ;
	v-p:north v-p:MainMenu ;
	v-p:south v-p:MainFooter ;
.
v-p:Page 
	rdf:type owl:Class ;
	rdfs:label "Page"@en ;
	rdfs:label "Страница"@ru ;
.
v-p:north
	rdf:type owl:ObjectProperty ;
	rdfs:domain v-p:Page ;
	rdfs:range rdfs:Resource ;
.
v-p:south
	rdf:type owl:ObjectProperty ;
	rdfs:domain v-p:Page ;
	rdfs:range rdfs:Resource ;
.
v-p:east
	rdf:type owl:ObjectProperty ;
	rdfs:domain v-p:Page ;
	rdfs:range rdfs:Resource ;
.
v-p:west
	rdf:type owl:ObjectProperty ;
	rdfs:domain v-p:Page ;
	rdfs:range rdfs:Resource ;
.
v-p:PageTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон для класса Страница"@ru ;
  rdfs:label "Template for Page class"@en ;
  v-ui:forClass v-p:Page ;
  v-ui:template """
<div class="page">
	<style>
		.north, .east, .west, .south {
			border: 1px solid red;
			min-height: 100px;
		}
		.center {
			border: 1px solid green;
			min-height: 100px;
		}
	</style>
	<div class="north" rel="v-p:north"></div>
	<div>
		<div class="east pull-left" rel="v-p:east"></div>
		<div class="center pull-left" id="main"></div>
		<div class="west pull-right" rel="v-p:west"></div>
		<div class="clearfix"></div>
	</div>
	<div class="south" rel="v-p:south"></div>
</div>
  """ ;
.
