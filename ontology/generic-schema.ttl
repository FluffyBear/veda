#-- Классы --


v-s:Dictionary
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Document ;
  rdfs:comment "Dictionary"@en ;
  rdfs:comment "Словарь"@ru ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "Dictionary"@en ;
  rdfs:label "Словарь"@ru ;
.
v-s:Document
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf v-s:Journaling ;
  rdfs:subClassOf v-s:Exportable ;
  rdfs:subClassOf v-s:Authorized ;
  rdfs:comment "A document"@en ;
  rdfs:comment "Документ"@ru ;
  rdfs:isDefinedBy v-s: ;
  rdfs:label "Document"@en ;
  rdfs:label "Документ"@ru ;
.
v-s:Level
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Dictionary ;
  rdfs:label "Level"@en ;
  rdfs:label "Уровень"@ru ;
.
v-s:YesNo
  rdf:type owl:Class ;
