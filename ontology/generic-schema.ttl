@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix v-wf: <http://semantic-machines.com/veda/veda-workflow/> .
@prefix mnd-s: <http://semantic-machines.com/veda/mondi-schema/> .
#временно перенесено

<http://semantic-machines.com/veda/generic-schema/>
  rdf:type owl:Ontology ;
  rdfs:label "Generic ontology."@en ;
  rdfs:label "Онтология Generic."@ru ;
  v-s:loadPriority 10 ;
.
#-- Классы --

v-s:Level
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Thing ;
  rdfs:label "Level"@en ;
  rdfs:label "Уровень"@ru ;
.
v-s:YesNo
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Thing ;
  rdfs:label "Yes No"@en ;
  rdfs:label "Да Нет"@ru ;
.
v-s:Bond
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Thing ;
  rdfs:label "Bond"@en ;
  rdfs:label "Связь"@ru ;
.

#-- Аттрибуты --


v-s:alternativeTitle
  rdf:type owl:DatatypeProperty ;
  rdfs:domain v-s:Level ;
  rdfs:label "Description"@en ;
  rdfs:label "Описание"@ru ;
  rdfs:range xsd:string ;
.


v-s:contactInfo
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Contact info"@en ;
  rdfs:label "Контактные данные"@ru ;
  rdfs:range xsd:string ;
.




v-s:decision
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Decision"@en ;
  rdfs:label "Решение"@ru ;
  rdfs:range xsd:string ;
.



v-s:hasAddDoc
  rdf:type owl:ObjectProperty ;
  rdfs:label "Add document"@en ;
  rdfs:label "Связанные документы"@ru ;
  rdfs:range v-s:Thing ;
.

v-s:hasChief
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:OrganizationUnit ;
  rdfs:label "Chief"@en ;
  rdfs:label "Непосредственный руководитель"@ru ;
  rdfs:range v-s:Appointment ;
.


v-s:hasFunctionalChief
  rdf:type owl:ObjectProperty ;
  rdfs:label "Functional chief"@en ;
  rdfs:label "Функциональный руководитель"@ru ;
  rdfs:range v-s:Appointment ;
.

v-s:member
  rdf:type owl:ObjectProperty ;
  rdfs:label "Member"@en ;
  rdfs:label "Участники"@ru ;
  rdfs:range v-s:Appointment ;
.

v-s:hasYesNo
  rdf:type owl:ObjectProperty ;
  rdfs:label "Yes no"@en ;
  rdfs:label "Да нет"@ru ;
  rdfs:range v-s:YesNo ;
.




#временно перенесено begin
v-s:registrationNumber
  v-s:hasNumerationMapper mnd-s:registrationNumberNumerationMapperMeeting;
#  v-s:hasNumerationMapper mnd-s:registrationNumberNumerationMapperAction;
.
#временно перенесено end




v-s:scope
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Scope"@en ;
  rdfs:label "Рамки"@ru ;
  rdfs:range xsd:string ;
.
v-s:alternativeScope
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Alternative scope"@en ;
  rdfs:label "Альтернативные рамки"@ru ;
  rdfs:range xsd:string ;
.
v-s:outOfScope
  rdf:type owl:DatatypeProperty ;
  rdfs:label "OutOfScope"@en ;
  rdfs:label "Не в ходит в рамки"@ru ;
  rdfs:range xsd:string ;
.
v-s:permission
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Permission"@en ;
  rdfs:label "Разрешение"@ru ;
  rdfs:range xsd:string ;
.

v-s:from
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:Bond ;
  rdfs:label "from"@en ;
  rdfs:label "от"@ru ;
  rdfs:range v-s:Thing ;
.
v-s:to
  rdf:type owl:ObjectProperty ;
  rdfs:domain v-s:Bond ;
  rdfs:label "to"@en ;
  rdfs:label "к"@ru ;
  rdfs:range v-s:Thing ;
.

