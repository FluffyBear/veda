.

mnd-s:code    
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialEntity ;
  rdfs:label "код"@ru ;
  rdfs:range xsd:string ;
.

mnd-s:postIndex    
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialEntity ;
  rdfs:label "почтовый индекс"@ru ;
  rdfs:range xsd:string ;
.

mnd-s:gninmb
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialEntity ;
  rdfs:label "GNINMB"@en ;
  rdfs:label "Код ИФНС"@ru ;
  rdfs:range xsd:string ;
.

mnd-s:uno
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialEntity ;
  rdfs:label "GNINMB"@en ;
  rdfs:label "Код территориального участка ИФНС"@ru ;
  rdfs:range xsd:string ;
.

mnd-s:ocatd
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialEntity ;
  rdfs:label "GNINMB"@en ;
  rdfs:label "Код ОКАТО"@ru ;
  rdfs:range xsd:string ;
.

mnd-s:status
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialEntity ;
  rdfs:label "STATUS"@en ;
  rdfs:label "Статус объекта"@ru ;
  rdfs:range xsd:string ;
.


mnd-s:AdministrativeTerritorialType
  rdf:type owl:Class ;
  rdfs:label "type of administrative-territorial entity"@en ;
  rdfs:label "типы административно-территориальный объектов"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.

mnd-s:abbreviation    
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialType ;
  rdfs:label "сокращенное название"@ru ;
  rdfs:range xsd:string ;
.

mnd-s:level
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialType ;
  rdfs:label "Уровень объекта данного типа"@ru ;
  rdfs:range xsd:integer ;
.

mnd-s:codeOfType
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeTerritorialType ;
  rdfs:label "Код типа объекта"@ru ;
  rdfs:range xsd:integer ;
.