# baseURI: http://semantic-machines.com/veda/mondi-schema
# imports: http://semantic-machines.com/veda/veda-schema
# imports: http://semantic-machines.com/veda/veda-ui

# c14n-version: 3
@prefix mondi-schema: <http://semantic-machines.com/veda/mondi-schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix veda-schema: <http://semantic-machines.com/veda/veda-schema/> .
@prefix veda-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
<http://semantic-machines.com/veda/mondi-schema>
  rdf:type owl:Ontology ;
  owl:imports <http://semantic-machines.com/veda/veda-schema> ;
  owl:imports <http://semantic-machines.com/veda/veda-ui> ;
  owl:versionInfo "1.0" ;
  rdfs:label "Mondi ontology"@en ;
  rdfs:label "Онтология Монди"@ru ;
.
mondi-schema:AdministrativeDocument
  rdf:type owl:Class ;
  rdfs:label "Administrative document"@en ;
  rdfs:label "Распорядительный документ"@ru ;
  rdfs:subClassOf veda-schema:Document ;
  rdfs:subClassOf veda-schema:Versioned ;
  rdfs:subClassOf mondi-schema:AdministrativeDocumentRestriction ;
.

mondi-schema:AdministrativeDocumentRestriction
  rdf:type owl:Restriction ;
  rdfs:label "AdministrativeDocumentRestriction"@en ;
  rdfs:label "Ограничение для Административного документа"@ru ;
  owl:onClass mondi-schema:AdministrativeDocumentType ;
  owl:onProperty mondi-schema:nomenclatureType ;
  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
.

mondi-schema:AdministrativeDocumentType
  rdf:type owl:Class ;
  rdfs:label "Document type"@en ;
  rdfs:label "Вид документа"@ru ;
  rdfs:subClassOf mondi-schema:DocumentType ;
.
mondi-schema:DocumentType
  rdf:type owl:Class ;
  rdfs:label "Document type"@en ;
  rdfs:label "Вид документа"@ru ;
  rdfs:subClassOf veda-schema:Dictionary ;
.
mondi-schema:RegistrationRecord
  rdf:type owl:Class ;
  rdfs:label "Registration record"@en ;
  rdfs:label "Регистрационная запись"@ru ;
  rdfs:subClassOf veda-schema:Document ;
.
mondi-schema:content
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mondi-schema:AdministrativeDocument ;
  rdfs:label "content"@en ;
  rdfs:label "содержание"@ru ;
  rdfs:range xsd:string ;
.
mondi-schema:nomenclatureType
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          mondi-schema:AdministrativeDocument
          mondi-schema:RegistrationRecord
        ) ;
    ] ;
  rdfs:label "document type"@en ;
  rdfs:label "вид документа"@ru ;
  rdfs:range mondi-schema:DocumentType ;
.
mondi-schema:registrationDate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mondi-schema:RegistrationRecord ;
  rdfs:label "Дата регистрации"@ru ;
  rdfs:range xsd:dateTime ;
.
mondi-schema:registrationNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mondi-schema:RegistrationRecord ;
  rdfs:label "Регистрационный номер"@ru ;
  rdfs:range xsd:string ;
.
mondi-schema:signer
  rdf:type owl:ObjectProperty ;
  rdfs:domain mondi-schema:AdministrativeDocument ;
  rdfs:label "signer"@en ;
  rdfs:label "подписант"@ru ;
  rdfs:range veda-schema:Appointment ;
.

mondi-schema:Dossier
  rdf:type owl:Class ;
  rdfs:label "Dossier"@en ;
  rdfs:label "Досье контрагента"@ru ;
  rdfs:subClassOf veda-schema:Document ;
  rdfs:subClassOf veda-schema:Versioned ;
  veda-schema:deprecatedId "6b0ea5c4957640c3bab5ab49b01eb01c" ;
.

mondi-schema:Action
  rdf:type owl:Class ;
  rdfs:label "Action"@en ;
  rdfs:label "Мероприятие"@ru ;
  rdfs:subClassOf veda-schema:Document ;
  rdfs:subClassOf veda-schema:Versioned ;
  veda-schema:deprecatedId "6b0ea5c4957640c3bab5ab49b01eb01c" ;
.

