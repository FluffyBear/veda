# baseURI: http://semantic-machines.com/veda/mondi-schema
# imports: http://semantic-machines.com/veda/veda-schema
# imports: http://semantic-machines.com/veda/veda-ui

# c14n-version: 3
@prefix mnd-s: <http://semantic-machines.com/veda/mondi-schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
<http://semantic-machines.com/veda/mondi-schema>
  rdf:type owl:Ontology ;
  rdfs:label "Mondi ontology"@en ;
  rdfs:label "Онтология Монди"@ru ;
  owl:imports <http://semantic-machines.com/veda/veda-schema> ;
  owl:imports <http://semantic-machines.com/veda/veda-ui> ;
  owl:versionInfo "1.0" ;
.
mnd-s:Action
  rdf:type owl:Class ;
  rdfs:label "Action"@en ;
  rdfs:label "Мероприятие"@ru ;
  rdfs:subClassOf v-s:Document ;
  rdfs:subClassOf v-s:Versioned ;
.
mnd-s:AddInfo
  rdf:type owl:Class ;
  rdfs:label "Add Info"@en ;
  rdfs:label "Доп информация"@ru ;
  rdfs:subClassOf v-s:Document ;
  rdfs:subClassOf v-s:Versioned ;
.
mnd-s:AddInfoKind
  rdf:type owl:Class ;
  rdfs:label "Add information classifier"@en ;
  rdfs:label "Классификатор видов дополнительной информации"@ru ;
  rdfs:subClassOf v-s:Document ;
.
mnd-s:AdministrativeDocument
  rdf:type owl:Class ;
  rdfs:label "Administrative document"@en ;
  rdfs:label "Распорядительный документ"@ru ;
  rdfs:subClassOf mnd-s:AdministrativeDocumentRestriction ;
  rdfs:subClassOf v-s:Document ;
  rdfs:subClassOf v-s:Versioned ;
.
mnd-s:AdministrativeDocumentRestriction
  rdf:type owl:Restriction ;
  rdfs:label "AdministrativeDocumentRestriction"@en ;
  rdfs:label "Ограничение для Административного документа"@ru ;
  owl:onClass mnd-s:AdministrativeDocumentType ;
  owl:onProperty mnd-s:nomenclatureType ;
  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
.
mnd-s:AdministrativeDocumentType
  rdf:type owl:Class ;
  rdfs:label "Document type"@en ;
  rdfs:label "Вид документа"@ru ;
  rdfs:subClassOf mnd-s:DocumentType ;
.
mnd-s:Area
  rdf:type owl:Class ;
  rdfs:label "Area"@en ;
  rdfs:label "Участок"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s:ClassifierLevel
  rdf:type owl:Class ;
  rdfs:label "Classifier of level"@en ;
  rdfs:label "Классификатор уровней"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s:DocumentType
  rdf:type owl:Class ;
  rdfs:label "Document type"@en ;
  rdfs:label "Вид документа"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s:Dossier
  rdf:type owl:Class ;
  rdfs:label "Dossier"@en ;
  rdfs:label "Досье контрагента"@ru ;
  rdfs:subClassOf v-s:Document ;
  rdfs:subClassOf v-s:Versioned ;
.
mnd-s:EmotionalCharacteristic
  rdf:type owl:Class ;
  rdfs:label "Positive. Negative"@en ;
  rdfs:label "Положительный. Отрицательный"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s:Mobility
  rdf:type owl:Class ;
  rdfs:label "Mobility"@en ;
  rdfs:label "Мобильность"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s:NormativeDocument
  rdf:type owl:Class ;
  rdfs:label "Normative Document"@en ;
  rdfs:label "Нормативный документ"@ru ;
  rdfs:subClassOf v-s:Document ;
  rdfs:subClassOf v-s:Versioned ;
.
mnd-s:Observation
  rdf:type owl:Class ;
  rdfs:label "Observation"@en ;
  rdfs:label "Наблюдение"@ru ;
  rdfs:subClassOf v-s:Document ;
  rdfs:subClassOf v-s:Versioned ;
.
mnd-s:RegistrationDate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:RegistrationRecord ;
  rdfs:label "Дата регистрации"@ru ;
  rdfs:range xsd:dateTime ;
.
mnd-s:RegistrationRecord
  rdf:type owl:Class ;
  rdfs:label "Registration record"@en ;
  rdfs:label "Регистрационная запись"@ru ;
  rdfs:subClassOf v-s:Document ;
.
mnd-s:content
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:AdministrativeDocument ;
  rdfs:label "content"@en ;
  rdfs:label "содержание"@ru ;
  rdfs:range xsd:string ;
.
mnd-s:descriptionOfPlace
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:Action ;
  rdfs:label "description of the place"@en ;
  rdfs:label "место проведения чего-либо"@ru ;
  rdfs:range xsd:string ;
.
mnd-s:hasAddInfo
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s:Action ;
  rdfs:label "has additional information"@en ;
  rdfs:label "ссылка на дополнительную информацию"@ru ;
  rdfs:range mnd-s:AddInfo ;
.
mnd-s:hasAddInfoKind
  rdf:type owl:ObjectProperty ;
  rdfs:label "add iformation klassifier"@en ;
  rdfs:label "виды дополнительной инфорамации"@ru ;
  rdfs:range mnd-s:AddInfoKind ;
.
mnd-s:hasAreaOfObservation
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s:Observation ;
  rdfs:label "area of observation"@en ;
  rdfs:label "участок наблюдения"@ru ;
  rdfs:range mnd-s:Area ;
.
mnd-s:hasCheckedDepartment
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s:Action ;
  rdfs:domain mnd-s:Observation ;
  rdfs:label "cheked department"@en ;
  rdfs:label "проверяемое подразделение"@ru ;
  rdfs:range v-s:Department ;
.
mnd-s:hasContractor
  rdf:type owl:ObjectProperty ;
  rdfs:label "contractor"@ru ;
  rdfs:label "подрядчик"@ru ;
  rdfs:range mnd-s:Dossier ;
.
mnd-s:hasMobility
  rdf:type owl:ObjectProperty ;
  rdfs:label "mobility"@en ;
  rdfs:label "мобильность"@ru ;
  rdfs:range mnd-s:Mobility ;
.
mnd-s:initiatorDepartment
  rdf:type owl:ObjectProperty ;
  rdfs:label "initiator department"@en ;
  rdfs:label "подразделение инициатор"@ru ;
  rdfs:range v-s:Department ;
.
mnd-s:nomenclatureType
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s:AdministrativeDocument ;
  rdfs:domain mnd-s:RegistrationRecord ;
  rdfs:label "document type"@en ;
  rdfs:label "вид документа"@ru ;
  rdfs:range mnd-s:DocumentType ;
.
mnd-s:registrationNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mnd-s:RegistrationRecord ;
  rdfs:label "Регистрационный номер"@ru ;
  rdfs:range xsd:string ;
.
mnd-s:responsibleOfObservation
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s:Observation ;
  rdfs:label "responsible of observation"@en ;
  rdfs:label "ответственное лицо замечания"@ru ;
  rdfs:range v-s:Appointment ;
.
mnd-s:risk
  rdf:type owl:DatatypeProperty ;
  rdfs:label "risk"@en ;
  rdfs:label "риск"@ru ;
  rdfs:range xsd:integer ;
.
mnd-s:signer
  rdf:type owl:ObjectProperty ;
  rdfs:domain mnd-s:AdministrativeDocument ;
  rdfs:label "signer"@en ;
  rdfs:label "подписант"@ru ;
  rdfs:range v-s:Appointment ;
.
v-s:comment
  rdfs:domain mnd-s:Observation ;
.
v-s:contributor
  rdfs:domain mnd-s:Action ;
.
v-s:controller
  rdfs:domain mnd-s:Action ;
.
v-s:dateFact
  rdfs:domain mnd-s:Action ;
  rdfs:domain mnd-s:Observation ;
.
v-s:dateFrom
  rdfs:domain mnd-s:Action ;
.
v-s:dateTo
  rdfs:domain mnd-s:Action ;
.
v-s:description
  rdfs:domain mnd-s:Action ;
  rdfs:domain mnd-s:Observation ;
.
v-s:hasParentLink
  rdfs:domain mnd-s:Action ;
.
v-s:hasStatusWorkflow
  rdfs:domain mnd-s:Action ;
.
v-s:registrationDate
  rdfs:domain mnd-s:Observation ;
.
v-s:registrationNumber
  rdfs:domain mnd-s:Observation ;
.
v-s:responsible
  rdfs:domain mnd-s:Action ;
.
