<http://semantic-machines.com/veda/mondi-schema-safety-audit>
  rdf:type owl:Ontology ;
  owl:imports <http://semantic-machines.com/veda/veda-schema> ;
  owl:imports <http://semantic-machines.com/veda/veda-ui> ;
  owl:imports <http://semantic-machines.com/veda/mondi-schema> ;
  owl:versionInfo "1.0" ;
  rdfs:label "Mondi ontology. Safety audit"@en ;
  rdfs:label "Онтология Монди. Аудит безопасности"@ru ;


mnd-s-sa:SafetyAudit
  rdf:type owl:Class ;
  rdfs:label "Safety audit"@en ;
  rdfs:label "Аудит безопасности"@ru ;
  rdfs:subClassOf v-s:Document ;
.
mnd-s-sa:SafetyStandardSection
  rdf:type owl:Class ;
  rdfs:label "Section of safety standard"@en ;
  rdfs:label "Вид надзора"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.
mnd-s-sa:SafetyAuditObservation
  rdf:type owl:Class ;
  rdfs:label "Observation of safety audit"@en ;
  rdfs:label "Замечание аудита безопасности"@ru ;
  rdfs:subClassOf v-s:Document ;
  rdfs:subClassOf v-s:Versioned ;
.
mnd-s-sa:SafetyAuditObservationKind
  rdf:type owl:Class ;
  rdfs:label "Kind of safety audit observation"@en ;
  rdfs:label "Вид Замечания аудита безопасности"@ru ;
  rdfs:subClassOf v-s:Dictionary ;
.

