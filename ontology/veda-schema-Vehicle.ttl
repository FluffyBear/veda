v-s:chassisNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Chassis number"@en ;
  rdfs:label "шасси (рама) №"@ru ;
  rdfs:range xsd:string ;
  rdfs:domain v-s:Pts ;
.
v-s:cabinNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Cabin number"@en ;
  rdfs:label "кузов (кабина,прицеп) №"@ru ;
  rdfs:range xsd:string ;
  rdfs:domain v-s:Pts ;
.
