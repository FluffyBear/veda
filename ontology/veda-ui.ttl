v-ui:FileTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон для файла"@ru ;
  rdfs:label "File template"@en ;
  v-ui:forClass v-s:File ;
  v-ui:template """
<div>
	<a href="/files/@"><span id="icon" class="label label-primary"></span> <span property="v-s:fileName" class="view -edit search"></span>, <span id="fileSize"></span></a>
</div>
<script>
	var fs = individual["v-s:fileSize"][0], 
		fn = individual["v-s:fileName"][0], 
		idx = fn.lastIndexOf("."), 
		ext = fn.substr(idx + 1);
	fs = fs >= 1073741824 ? (fs / 1073741824).toFixed(1) + " Gb" :
	     fs >= 1048576 ? (fs / 1048576).toFixed(1) + " Mb" :
	     fs >= 1024 ? (fs / 1024).toFixed(1) + " Kb" :
	     fs + " bytes";
	$("span#fileSize", template).text(fs);
	$("span#icon", template).text(ext);
</script>
  """ ;
.
