v-ui:ClassTemplateTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон для класса -Шаблон класса-"@ru ;
  rdfs:label "Template for -Class template- class"@en ;
  v-ui:forClass v-ui:ClassTemplate ;
  v-ui:template """
<div class="panel panel-default">
  <div class="panel-heading">
    <div class="row">
      <div class="col-md-4">
        <span about="v-ui:ClassTemplate" property="rdfs:label"></span>
      </div>
      <div class="col-md-8 text-right text-muted">
        <a href="#/@"><span property="@"></span></a>
      </div>  
    </div>
  </div>
  <div class="panel-body">
    <strong about="rdfs:label" property="rdfs:label" class="text-muted -view edit search"></strong>
    <h3 property="rdfs:label"></h3>
    <veda-control property="rdfs:label" type="multilingualString" class="-view edit search"></veda-control>
	<br class="-view edit search">
    <strong about="rdfs:comment" property="rdfs:label" class="text-muted -view edit search"></strong>
    <div><em property="rdfs:comment"></em></div>
    <veda-control property="rdfs:comment" type="multilingualString" class="-view edit search"></veda-control>
    <br class="-view edit search">
	<strong about="v-ui:forClass" property="rdfs:label" class="text-muted"></strong>
	<div rel="v-ui:forClass" template="v-ui:ClassNameLabelTemplate"></div>
	<veda-control rel="v-ui:forClass" type="link" class="-view edit search fullsearch fulltext dropdown"></veda-control>
	<br>
	<veda-control property="v-ui:template" type="source"></veda-control>
  </div>
  <div class="panel-footer">
    <button type="button" class="action btn btn-primary view -edit -search" id="edit" about="v-b:Edit" property="rdfs:label"/>
    <button type="button" class="action btn btn-success -view edit -search" id="save" about="v-b:Save" property="rdfs:label"/>
    <button type="button" class="action btn btn-default -view edit -search" id="cancel" about="v-b:Cancel" property="rdfs:label"/>
    <button type="button" class="action btn btn-link view edit -search" id="delete" about="v-b:Delete" property="rdfs:label"/>
  </div>
</div>
  """ ;
.

