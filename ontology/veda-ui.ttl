  rdfs:label "Template for Sort order class"@en ;
  v-ui:forClass v-s:SortOrder ;
  v-ui:embedded "true"^^xsd:boolean ;
  v-ui:template """
	<div class="panel panel-default">
		<!--div class="panel-heading">
		</div-->
		<div class="panel-body">
			<div class="row">
				<div class="col-md-8">
					<span>Свойство:</span>
					<div rel="v-s:orderBy" template="v-ui:LabelTemplate">Свойство:</div>
				</div>
				<div class="col-md-4">
					<span>Направление:</span>
					<div rel="v-s:direction" template="v-ui:LabelTemplate">Направление</div>
				</div>
			</div>
		</div>
	</div>
  """ ;
