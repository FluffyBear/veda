				<div rel="v-s:sort" template="v-ui:SortOrderTemplate">Сортировки</div>
			</div>
		</div>
		<br/>
		<button type="button" class="action btn btn-primary" id="search">Найти</button>
        <button type="button" class="action btn btn-primary" id="edit">Редактировать</button>
        <button type="button" class="action btn btn-success" id="save">Сохранить</button>
