v-ui:JournalTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон журнала"@ru ;
  rdfs:label "Journal template"@en ;
  v-ui:forClass v-s:Journal ;
  v-ui:template """
<div class="container-fluid journal">
    <style>
      div[rel="v-s:childRecord"]:first-child > div.journal-record > hr {display:none;}
      div.sub-journal {margin-top: 10px;}
      .journal-record > div > .sub-journal {margin-left: 5px; padding-left: 20px; border-left: 1px solid #ddd;}
      .journal hr.margin-sm {margin: 10px 0px;}
    </style>
    <h2 about="v-s:Journal" property="rdfs:label"></h2>
    <h4 id="docLnk">
      <span about="v-wf:onDocument" property="rdfs:label"></span>: 
      <span rel="v-wf:onDocument" template="v-ui:ClassNameLabelTemplate"></span>
    </h4>
    <h4 id="procLnk">
      <span about="v-wf:onProcess" property="rdfs:label"></span>: 
      <span rel="v-wf:onProcess" template="v-ui:ClassNameLabelTemplate"></span>
    </h4>
    <div class="panel panel-default">
		<div class="panel-heading">
			<div class="row">
				<div class="col-md-3 col-sm-3">
					<strong about="v-s:JournalRecord" property="rdfs:label"></strong>
				</div>
				<div class="col-md-7 col-sm-6">
					<strong about="v-s:description" property="rdfs:label"></strong>
				</div>
				<div class="col-md-2 col-sm-3 text-right">
					<strong about="v-s:created" property="rdfs:label"></strong>
				</div>
			</div>
		</div>
		<div class="panel-body">
			<div id="records">
				<div rel="v-s:childRecord"></div>
			</div>
		</div>
	</div>
</div>
<script>
  if (!individual.hasValue("v-wf:onProcess")) $("#procLnk").remove();
  if (!individual.hasValue("v-wf:onDocument")) $("#docLnk").remove();
</script>
  """ ;
.
