# baseURI: http://semantic-machines.com/veda/veda-ui
# imports: http://semantic-machines.com/veda/veda-schema
# imports: http://www.w3.org/1999/02/22-rdf-syntax-ns#
# imports: http://www.w3.org/2000/01/rdf-schema#
# imports: http://www.w3.org/2002/07/owl

# c14n-version: 3
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix veda-schema: <http://semantic-machines.com/veda/veda-schema/> .
@prefix veda-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
<http://semantic-machines.com/veda/veda-ui>
  rdf:type owl:Ontology ;
  rdfs:label "Veda system user interface ontology"@en ;
  rdfs:label "Онтология пользовательского интерфейса системы Веда"@ru ;
  owl:imports <http://semantic-machines.com/veda/veda-schema> ;
  owl:imports rdf: ;
  owl:imports rdfs: ;
  owl:imports <http://www.w3.org/2002/07/owl> ;
  owl:versionInfo "1.0" ;
.
veda-schema:EN
  rdf:type veda-ui:Language ;
  rdfs:label "English"@en ;
  rdfs:label "Английский"@ru ;
.
veda-schema:RU
  rdf:type veda-ui:Language ;
  rdfs:label "Russian"@en ;
  rdfs:label "Русский"@ru ;
.
veda-schema:preferredLanguage
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda-ui:Preferences ;
  rdfs:label "preferred language"@en ;
  rdfs:label "предпочитаемый язык"@ru ;
  rdfs:range veda-ui:Language ;
.
veda-ui:Language
  rdf:type owl:Class ;
  rdfs:label "Language"@en ;
  rdfs:label "Язык"@ru ;
  rdfs:subClassOf owl:Thing ;
.
veda-ui:Preferences
  rdf:type owl:Class ;
  rdfs:label "Preferences"@en ;
  rdfs:label "Предпочтения"@ru ;
  rdfs:subClassOf owl:Thing ;
.
veda-ui:hasPreferences
  rdf:type owl:ObjectProperty ;
  rdfs:domain veda-schema:Person ;
  rdfs:label "preferences"@en ;
  rdfs:label "предпочтения"@ru ;
  rdfs:range veda-ui:Preferences ;
.
