  rdfs:label "Шаблон для класса Рабочее задание"@ru ;
  rdfs:label "Template for Work order class"@en ;
  v-ui:forClass v-wf:Variable ;
  v-ui:embedded "true"^^xsd:boolean ;
  v-ui:template """
  <div>
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-md-6">
	        <span about="v-wf:Variable" property="rdfs:label">Имя класса</span>
  	      </div>  
          <div class="col-md-6 text-right text-muted">
            <a href="id"><span property="id">ID</span></a>
	      </div>
	    </div>
      </div>
      <div class="panel-body">
        <strong class="text-muted" about="rdfs:label" property="rdfs:label"></strong>
        <h3 property="rdfs:label">Наименование</h3>
        <br />
        <div class="row">
          <div class="col-md-6">
            <strong class="text-muted" about="v-wf:variableName" property="rdfs:label"></strong>
