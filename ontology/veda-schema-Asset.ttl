@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix v-wf: <http://semantic-machines.com/veda/veda-workflow/> .

<http://semantic-machines.com/veda/veda-schema-Asset/>
  rdf:type owl:Ontology ;
  rdfs:label "Veda system ontology. Asset"@en ;
  rdfs:label "Онтология системы Veda. Основное средство"@ru ;
  v-s:loadPriority 11 ;
.
#--------------------------------------------------
v-s:Asset
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:UserThing ;
  rdfs:subClassOf v-s:Embedded ;
  rdfs:subClassOf v-s:Registered ;
  rdfs:label "Asset"@en ;
  rdfs:label "Основное средство"@ru ;
.
v-s:assetNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Fixed asset ID"@en ;
  rdfs:label "Номер ОС"@ru ;
  rdfs:range xsd:string ;
  rdfs:domain v-s:Asset ;
.
v-s:vinNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Fixed asset VIN"@en ;
  rdfs:label "Идентификатор VIN"@ru ;
  rdfs:range xsd:string ;
  rdfs:domain v-s:Asset ;
.

mnd-s:PsAssetNumber
  rdf:type v-ui:DatatypePropertySpecification ;
  v-ui:forClass v-s:Asset ;
  v-ui:forProperty v-s:assetNumber ;
  v-ui:maxCardinality 1 ;
  v-ui:minCardinality 0 ;
  v-ui:tooltip "non-required, single value."@en ;
  v-ui:tooltip "Необязательно к заполнению, единичное значение."@ru ;
  rdfs:label "Some property specification for Asset class"@en ;
  rdfs:label "Спецификация некоторых cвойств для класса Asset"@ru ;
.