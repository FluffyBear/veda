    if (!individual['rdf:type']) return '';
	return "numeration-scope:"+(new Date()).getFullYear()+((typeof window === "undefined")?individual['rdf:type'][0].data:individual['rdf:type'][0].id);
})
""" ;
  v-s:numerationGetNextValue """  
