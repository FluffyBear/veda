extends generic

block content
	- void treeNode(string uri)
		a(href="/individuals/#{uri}") #{uri}
		ol(style="list-style:inherit;")
			- string[] tmp = subClasses.get(uri, string[].init);
			- foreach (_subClass; tmp)
				li 
					- treeNode(_subClass);

	h1 Class tree:
	ol(style="list-style:none;")
		li
			- treeNode("owl:Thing");